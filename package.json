{
  "name": "remark-docx",
  "version": "0.2.0",
  "description": "remark plugin to compile markdown to docx (Microsoft Word, Office Open XML).",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && tsc -p tsconfig-build.json && gzip -c dist/index.js | wc -c",
    "clean": "rm -rf dist",
    "dev": "tsup --watch && tsc -p tsconfig-build.json -w",
    "test": "vitest run",
    "tsc": "tsc -p . --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook -- --no-open\" \"wait-on tcp:6006 && test-storybook\"",
    "typedoc": "typedoc",
    "prepublishOnly": "npm run typedoc && rimraf lib && npm run build"
  },
  "dependencies": {
    "docx": "^9.3.0",
    "mdast2docx": "^0.3.5",
    "tsup": "^8.4.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@storybook/addon-storysource": "^8.6.10",
    "@storybook/react": "^8.6.10",
    "@storybook/react-vite": "^8.6.10",
    "@storybook/source-loader": "^8.6.10",
    "@storybook/test-runner": "^0.22.0",
    "@types/adm-zip": "^0.5.7",
    "@types/file-saver": "^2.0.7",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/unist": "^3.0.3",
    "adm-zip": "^0.5.16",
    "concurrently": "^9.1.2",
    "docx-preview": "^0.3.5",
    "file-saver": "^2.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-is": "^19.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "rimraf": "^6.0.1",
    "storybook": "^8.6.10",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.6.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vitest": "^3.0.9",
    "wait-on": "^8.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/remark-docx.git"
  },
  "keywords": [
    "unist",
    "remark",
    "mdast",
    "markdown",
    "docx",
    "word",
    "office",
    "openxml",
    "latex",
    "math"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "contributors": [
    {
      "name": "inokawa",
      "url": "https://github.com/inokawa"
    },
    {
      "name": "Mayank Kumar Chaudhari",
      "url": "https://github.com/mayank1513"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/remark-docx/issues"
  },
  "homepage": "https://github.com/inokawa/remark-docx#readme"
}
